{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "baseUrl": "./src",
    "paths": {
      "@graphql/*": ["graphql/*"],
      "@middleware/*": ["middleware/*"],
      "@models/*": ["models/*"],
      "@utils/*": ["utils/*"]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
